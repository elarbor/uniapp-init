{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path": "pages/requset/users",
			"style": {
				"navigationBarTitleText": "发请求案例"
			}
		}
	],
	"subPackages": [{
			"root": "pages/status",
			"pages": [{
				"path": "abnormal/index",
				"style": {
					"navigationBarTitleText": "异常告知"
				}
			}]
		},
		{
			"root": "pages/demo",
			"pages": [
				{
					"path": "vue3",
					"style": {
						"navigationBarTitleText": "vue3语法展示"
					}
				},
				{
					"path": "pinia",
					"style": {
						"navigationBarTitleText": "pinia展示"
					}
				}
			]
		}
	],
	"tabBar": {
		"list": [{
				"pagePath": "pages/index/index",
				"iconPath": "static/tabbar/maintain-0.png",
				"selectedIconPath": "static/tabbar/maintain-1.png",
				"text": "首页"
			},
			{
				"pagePath": "pages/requset/users",
				"iconPath": "static/tabbar/commercial-0.png",
				"selectedIconPath": "static/tabbar/commercial-1.png",
				"text": "请求"
			}
		]
	},
	"easycom": {
		"custom": {
			"^u--(.*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue",
			"^up-(.*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue",
			"^u-([^-].*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue"
		}
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
}